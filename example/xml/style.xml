<?xml version="1.0"?>
<Figure>
  <Name>test.eps</Name>
  <Terminal>postscript eps enhanced color</Terminal>
  <Margins right="0.1" left="0.2" bottom="0.2" top="0.1" h="0.0" v="0.0"></Margins>
  <Row>1</Row>
  <Col>1</Col>
  <Fonts>
    <Font name="Helvetica" size="16">xlabel</Font>
    <Font name="Helvetica" size="16">ylabel</Font>
    <Font name="Helvetica" size="16">title</Font>
    <Font name="Helvetica" size="12">key</Font>
    <Font name="Helvetica" size="12">xtics</Font>
    <Font name="Helvetica" size="12">ytics</Font>
  </Fonts>
  <Panels>
    <Panel>
      <Opt>
        <Sample>1000</Sample>
        <Grid></Grid>
        <Key></Key>
        <Xaxis show="true" min="0" max="10" log="false">
          <Name>x</Name>
          <Format tics="5" mtics="2">%g</Format>
          <Label>
            <text>V_{LSR} (km/s)</text>
            <offset>
              <x system="character">0</x>
              <y system="character">0</y>
            </offset>
          </Label>
        </Xaxis>
        <Yaxis show="true" min="0" max="10" log="false">
          <Name>y</Name>
          <Format tics="5" mtics="2">%g</Format>
          <Label>
            <text>T_{MB} (K)</text>
            <offset>
              <x system="character">0</x>
              <y system="character">0</y>
            </offset>
          </Label>
        </Yaxis>
      </Opt>
      <Data>
        <Name>test.dat</Name>
        <Opt>
          <isFunc>false</isFunc>
          <using>1:2</using>
          <index>0</index>
          <with>line</with>
          <lineStyle lw="1" lt="1" lc="#000000" pt="" ps="" dt=""></lineStyle>
          <title></title>
        </Opt>
      </Data>
      <Annotation>
        <arrow hideHead="true">
          <lineStyle lw="1" lt="1" lc="#000000" pt="" ps="" dt=""></lineStyle>
          <from>
            <x system="graph">0.5</x>
            <y system="graph">0.0</y>
          </from>
          <to>
            <x system="graph">0.5</x>
            <y system="graph">1.0</y>
          </to>
        </arrow>
        <label>
          <text>label</text>
          <location>
            <x system="graph">0.5</x>
            <y system="graph">0.5</y>
          </location>
        </label>
      </Annotation>
    </Panel>
  </Panels>
</Figure>